<?xml version="1.0"?>
<form xmlns="http://www.demandware.com/xml/form/2008-04-19">

    <field

        formid="productId"

        label="productId"

        type="string"

        mandatory="true"

        binding="productId"

    />
 <!-- binding="productID" -->
    <field

        formid="phoneNumbers"

        label="phoneNumbers"

        mandatory="true"
        
        binding="phoneNumbers"

        min-length="12"

        max-length="15"
        
        regexp="/^\(?([0-9]{3})\)?[-. ]?([0-9]{3})[-. ]?([0-9]{6})*$/"
        
		type="string"
        
        missing-error="error.message.required"

        parse-error="error.message.parse.phone.NotifyMeBackInStock.form"
        
        range-error="error.message.between10and20"
        /> 

    <action formid="subscribe" label="subscribe" valid-form="true"/>

</form>